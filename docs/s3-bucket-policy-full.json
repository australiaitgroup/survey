{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadMainApp",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::jr-sigma-survey-prod/*",
      "Condition": {
        "StringNotLike": {
          "s3:prefix": [
            "backup/*",
            "private/*"
          ]
        }
      }
    }
  ]
}
